{
  "name": "website-portfolio",
  "version": "1.0.0",
  "description": "Portfolio Website",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "set NODE_ENV=production&& next start",
    "build:test": "set NODE_ENV=test&& next build",
    "start:test": "set NODE_ENV=test&& next start",
    "cypress:open": "env-cmd -f .env.test.local ./node_modules/.bin/cypress open",
    "cypress:start": "start-server-and-test 'npm run start:test' 3000 cypress:open",
    "cypress:build": "npm run build:test && npm run cypress:start",
    "lint": "next lint",
    "test": "jest --watch --verbose --detectOpenHandles",
    "coverage": "jest --watch --verbose --detectOpenHandles --coverage",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run lint && ((npm run check-format && npm run check-lint && npm run check-types && node -e \"require('colors'); console.log('LINT SUCCESSFULLY COMPLETED.'.green)\") || (npm run format && node -e \"require('colors'); console.log('\\n\\n', 'WARNING: FILE CHANGES:'.red, 'Please add to staging.'); process.exit(1)\"))"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbuzzerio/website-portfolio.git"
  },
  "author": "Nicholas Buzzerio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nbuzzerio/website-portfolio/issues"
  },
  "homepage": "https://github.com/nbuzzerio/website-portfolio#readme",
  "dependencies": {
    "@hookform/resolvers": "^2.9.6",
    "jsonwebtoken": "^8.5.1",
    "lottie-react": "^2.3.1",
    "mongoose": "^6.5.1",
    "next": "^14.1.0",
    "next-sitemap": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "sass": "^1.52.1",
    "sharp": "^0.34.1",
    "start-server-and-test": "^1.14.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/jquery": "^3.5.14",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "autoprefixer": "^10.4.7",
    "colors": "^1.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "msw": "^0.44.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.4"
  }
}
